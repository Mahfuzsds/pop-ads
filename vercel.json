{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "build/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/build/client/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/server.js"
    }
  ]
}
